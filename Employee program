package employee;
import java.util.Scanner;
 
public class Employee {
    String name;
    int Yearofjoining;
    String address;
    Employee(String n,int y,String a)
    {
        name=n;
        Yearofjoining=y;
        address=a;
    }
    void display()
    {
        System.out.println(name+"\t\t"+Yearofjoining+"\t\t"+address);
      
    }

    public static void main(String[] args) {
        
       Employee ob1=new Employee("Robert",1994,"64C-WallsStreet");
       Employee ob2=new Employee("Sam",2000,"68D-WallsStreet");
       Employee ob3=new Employee("John",1999,"26B-WallsStreet");
       System.out.println("Name\t\t"+"Yearofjoining\t\t"+"Address\t\t");
       ob1.display();
       ob2.display();
       ob3.display();
    }
}
